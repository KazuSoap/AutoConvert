          <p class="form check half"><input type="checkbox" id="config_preset_check" /><label for="config_preset_check">プリセット</label><select id="config_preset" disabled="disabled"></select></p>
          <p class="check third"><input type="checkbox" id="config_reset" class="indeterminate" /><label for="config_reset">リセット</small></label></p>
